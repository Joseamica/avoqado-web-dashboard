{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.tsx",
    "src/App.tsx",
    "vite.config.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx}"
  ],
  "ignore": [
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "dist/**",
    "build/**"
  ],
  "ignoreDependencies": [
    "@types/*",
    "typescript",
    "vite",
    "@vitejs/plugin-react",
    "tailwindcss",
    "postcss",
    "autoprefixer"
  ],
  "ignoreBinaries": [],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": true,
  "typescript": {
    "config": ["tsconfig.json", "tsconfig.node.json"]
  }
}
